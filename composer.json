{
    "name": "shawm11/oz-auth",
    "description": "PHP implementation of the Oz web authorization protocol",
    "type": "library",
    "license": "MIT",
    "version": "1.1.4",
    "require": {
        "php": ">=8.1",
        "nategood/httpful": "^1.0",
        "shawm11/hawk-auth": "^1.0",
        "shawm11/iron-crypto": "^1.0"
    },
    "require-dev": {
        "php": ">=8.1",
        "codeception/assert-throws": "^1.0",
        "codeception/specify": "^2.0",
        "codeception/verify": "^3.2",
        "consolidation/robo": "^4.0",
        "marcocesarato/php-conventional-changelog": "^1.17",
        "mockery/mockery": "^1.1",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Shawm11\\Oz\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shawm11\\Oz\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
