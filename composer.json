{
    "name": "shawm11/oz-auth",
    "description": "PHP implementation of the Oz web authorization protocol",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "require": {
        "php": ">=5.6.0",
        "nategood/httpful": "^0.2.20",
        "shawm11/hawk-auth": "^1.0",
        "shawm11/iron-crypto": "^1.0"
    },
    "require-dev": {
        "php": ">=7.1.0",
        "codeception/assert-throws": "^1.0",
        "codeception/specify": "^1.1",
        "codeception/verify": "^1.0",
        "consolidation/robo": "^1.3",
        "mockery/mockery": "^1.1",
        "phpstan/phpstan": "^0.11.12",
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.2",
        "vierbergenlars/php-semver": "^3.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Shawm11\\Oz\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shawm11\\Oz\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
